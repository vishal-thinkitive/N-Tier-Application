@model List<GlobalEntity.Student>
@{
    ViewData["Title"] = "Index";
}
<div class="container p-3">
    <div class="row p-4">
        <div class=" col-6">
            <h2 class=" text-secondary">Student List</h2>
        </div>
        

    </div>
    <table class="table table-bordered table-striped table-success" style="width:100%">
        <thead>
            <tr>
                <th>
                    Name
                </th>
                <th>
                    PhoneNo
                </th>
                <th>
                    IdProof
                </th>
                <th>
                    Member Id
                </th>
                <th>
                    Joining date
                </th>
                <th>
                    Update or Remove
                </th>

            </tr>
        </thead>
        <tbody>
            @foreach (var obj in Model)
            {
                <tr>
                    <td>
                        @obj.Name
                    </td>
                    <td>
                        @obj.Phone
                    </td>
                    <td>
                        @obj.IdProof
                    </td>
                    <td>
                        @obj.MemberId
                    </td>
                    <td>
                        @obj.JoiningDate
                    </td>
                    
                </tr>
            }
        </tbody>
    </table>

</div>

